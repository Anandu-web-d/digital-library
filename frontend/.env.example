# Frontend Environment Variables
# Copy this file to .env.local for local development

# Backend API URL (leave empty for development - Vite proxy handles it)
# In production, set this to your Render backend URL
VITE_API_URL=

# Backend URL for static files (uploads, PDFs)
# In production, set this to your Render backend URL (e.g., https://digital-library-backend.onrender.com)
VITE_BACKEND_URL=http://localhost:5000

# AI Service URL (if you deploy the Python AI service separately)
VITE_AI_SERVICE_URL=http://localhost:8000
